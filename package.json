{
  "name": "moon",
  "version": "0.0.1",
  "main": "dist/electron/main.js",
  "author": {
    "name": "liangshen001",
    "email": "wangliang520wl@hotmail.com"
  },
  "bugs": {
    "url": "https://github.com/liangshen001/moon/issues"
  },
  "description": "moon",
  "homepage": "https://github.com/liangshen001/moon#readme",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "dev": "ng test",
    "e2e": "ng e2e",
    "ngx-electron:server": "ngx-electron server-start . --hmr",
    "ngx-electron:local": "ngx-electron local-start .",
    "build": "ng build",
    "build:dev": "npm run build -- --configuration=dev",
    "build:prod": "npm run build -- --configuration=production",
    "build:linux": "npm run build:prod && npx electron-builder build --linux",
    "build:win": "ngx-electron build . -m",
    "build:windows:dev": "npm run build:electron && npm run build:dev && npx electron-builder build --windows",
    "build:mac": "npm run build:electron && npm run build:prod && npx electron-builder build --mac",
    "npm:scope:ngx-electron": "npm config set scope ngx-electron",
    "build:ngx-element-resizable": "ng build ngx-element-resizable",
    "build:ngx-row": "ng build ngx-row",
    "build:@ngx-electron/core": "ng build @ngx-electron/core",
    "build:@ngx-electron/data": "ng build @ngx-electron/data",
    "build:@ngx-electron/main": "tsc -p ./projects/ngx-electron/main && npm link ./projects/ngx-electron/main",
    "build:@ngx-electron/cli": "tsc -p ./projects/ngx-electron/cli && npm link ./projects/ngx-electron/cli",
    "build:ngx-message-editor": "ng build ngx-message-editor",
    "build:ngx-mat-dynamic-form": "ng build ngx-mat-dynamic-form",
    "publish:ngx-mat-dynamic-form": "npm run build:ngx-mat-dynamic-form && npm publish dist/ngx-mat-dynamic-form",
    "publish:ngx-element-resizable": "npm run build:ngx-element-resizable && npm publish dist/ngx-element-resizable",
    "publish:ngx-message-editor": "npm run build:ngx-message-editor && npm publish dist/ngx-message-editor",
    "publish:ngx-row": "npm run build:ngx-row && npm publish dist/ngx-row",
    "publish:@ngx-electron/core": "npm run build:@ngx-electron/core && npm run npm:scope:ngx-electron && npm publish --access public dist/ngx-electron/core",
    "publish:@ngx-electron/data": "npm run build:@ngx-electron/data && npm run npm:scope:ngx-electron && npm publish --access public dist/ngx-electron/data",
    "publish:@ngx-electron/main": "npm run build:@ngx-electron/main && npm run npm:scope:ngx-electron && npm publish --access public projects/ngx-electron/main",
    "publish:@ngx-electron/cli": "npm run build:@ngx-electron/cli && npm run npm:scope:ngx-electron && npm publish --access public projects/ngx-electron/cli"
  },
  "peerDependencies": {},
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "~7.1.1",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@ctrl/ngx-emoji-mart": "^0.6.3",
    "@ngrx/db": "^2.2.0-beta.0",
    "@ngrx/effects": "6.0.1",
    "@ngrx/entity": "6.0.1",
    "@ngrx/router-store": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@stomp/ng2-stompjs": "^6.0.0",
    "angular-in-memory-web-api": "0.6.0",
    "angular2-virtual-scroll": "^0.3.1",
    "commander": "^2.19.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngrx-store-freeze": "^0.2.3",
    "ngx-color-picker": "^6.6.0",
    "ngx-quill": "^3.4.0",
    "quill": "^1.3.6",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26",
    "@angular-devkit/build-angular": "^0.12.2",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "4.0.0",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
