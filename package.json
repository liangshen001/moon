{
    "name": "moon",
    "version": "0.0.1",
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "start": "ngx-electron . --server --hmr",
        "start2": "ng serve --port 4200",
        "start:electron": "npm run build:electron && ng build && electron .",
        "start:local": "npm-run-all -p ng:serve electron:serve",
        "start:prod": "npm-run-all -p ng:serve  -- --configuration=production electron:serve",
        "build": "npm run build:electron && ng build",
        "build:dev": "npm run build -- --configuration=dev",
        "build:prod": "npm run build -- --configuration=production",
        "build:electron": "tsc -p electron/tsconfig.electron.json",
        "ng:serve": "ng serve --port 4200",
        "ng:serve:web": "npm run postinstall:electron && ng serve --port 4200",
        "electron:serve": "wait-on http-get://localhost:4200/ && npm run build:electron && electron . --server --port 4200 --host localhost",
        "electron:local": "npm run build && electron .",
        "electron:local:dev": "npm run build:dev && electron .",
        "electron:local:prod": "npm run build:prod && electron . --prod",
        "build:linux": "npm run build:prod && npx electron-builder build --linux",
        "build:windows": "npm run build:prod && npx electron-builder build --windows",
        "electron:windows:dev": "npm run build:dev && npx electron-builder build --windows",
        "build:mac": "npm run build:prod && npx electron-builder build --mac",
        "dev": "npm run postinstall:web && ng test",
        "e2e": "npm run postinstall:web && ng e2e",
        "npm:scope:ngx-electron": "npm config set scope ngx-electron",
        "ngx-element-resizable:build": "ng build ngx-element-resizable",
        "ngx-row:build": "ng build ngx-row",
        "@ngx-electron/core:build": "ng build @ngx-electron/core",
        "@ngx-electron/data:build": "ng build @ngx-electron/data",
        "@ngx-electron/main:build": "tsc -p ./projects/ngx-electron/main && npm link ./projects/ngx-electron/main",
        "@ngx-electron/cli:build": "tsc -p ./projects/ngx-electron/cli && npm link ./projects/ngx-electron/cli",
        "ngx-message-editor:build": "ng build ngx-message-editor",
        "ngx-mat-dynamic-form:build": "ng build ngx-mat-dynamic-form",
        "ngx-mat-dynamic-form:publish": "npm run ngx-mat-dynamic-form:build && npm publish dist/ngx-mat-dynamic-form",
        "ngx-element-resizable:publish": "npm run ngx-element-resizable:build && npm publish dist/ngx-element-resizable",
        "ngx-message-editor:publish": "npm run ngx-message-editor:build && npm publish dist/ngx-message-editor",
        "ngx-row:publish": "npm run ngx-row:build && npm publish dist/ngx-row",
        "@ngx-electron/core:publish": "npm run @ngx-electron/core:build && npm run npm:scope:ngx-electron && npm publish --access public dist/ngx-electron/core",
        "@ngx-electron/data:publish": "npm run @ngx-electron/data:build && npm run npm:scope:ngx-electron && npm publish --access public dist/ngx-electron/data",
        "@ngx-electron/main:publish": "npm run @ngx-electron/main:build && npm run npm:scope:ngx-electron && npm publish --access public dist/ngx-electron/main"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~7.0.0",
        "@angular/cdk": "~7.1.1",
        "@angular/common": "~7.0.0",
        "@angular/compiler": "~7.0.0",
        "@angular/core": "~7.0.0",
        "@angular/forms": "~7.0.0",
        "@angular/http": "~7.0.0",
        "@angular/material": "^7.1.1",
        "@angular/platform-browser": "~7.0.0",
        "@angular/platform-browser-dynamic": "~7.0.0",
        "@angular/router": "~7.0.0",
        "@ctrl/ngx-emoji-mart": "^0.6.3",
        "@ngrx/db": "^2.2.0-beta.0",
        "@ngrx/effects": "6.0.1",
        "@ngrx/entity": "6.0.1",
        "@ngrx/router-store": "6.0.1",
        "@ngrx/store": "6.0.1",
        "@ngrx/store-devtools": "6.0.1",
        "@ngx-translate/core": "10.0.2",
        "@ngx-translate/http-loader": "3.0.1",
        "@stomp/ng2-stompjs": "^6.0.0",
        "angular-in-memory-web-api": "0.6.0",
        "angular2-virtual-scroll": "^0.3.1",
        "commander": "^2.19.0",
        "core-js": "^2.5.4",
        "dotenv": "latest",
        "hammerjs": "^2.0.8",
        "ngrx-store-freeze": "^0.2.3",
        "ngx-color-picker": "^6.6.0",
        "ngx-quill": "^3.4.0",
        "quill": "^1.3.6",
        "rxjs": "~6.3.3",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.10.0",
        "@angular-devkit/build-ng-packagr": "~0.10.0",
        "@angular/cli": "~7.0.6",
        "@angular/compiler-cli": "~7.0.0",
        "@angular/language-service": "~7.0.0",
        "@angularclass/hmr": "^2.1.3",
        "@types/dotenv": "^6.1.0",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "electron": "2.0.3",
        "electron-builder": "20.14.7",
        "electron-reload": "1.2.2",
        "electron-updater": "^3.1.2",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^4.2.0",
        "npm-run-all": "4.1.3",
        "npx": "10.2.0",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tsickle": ">=0.29.0",
        "tslib": "^1.9.0",
        "tslint": "~5.11.0",
        "typescript": "~3.1.6",
        "wait-on": "^2.1.0"
    }
}
